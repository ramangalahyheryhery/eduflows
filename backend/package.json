{
  "name": "eduflows-backend",
  "version": "1.0.0",
  "description": "Backend API pour EduFlows",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "docker:dev": "cd .. && docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "cd .. && docker-compose up --build",
    "docker:down": "cd .. && docker-compose down",
    "docker:clean": "cd .. && docker-compose down -v --remove-orphans",
    "docker:logs": "cd .. && docker-compose logs -f",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["education", "api", "nodejs"],
  "author": "Votre Ã‰quipe",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}

